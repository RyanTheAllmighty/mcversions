{
  "name": "mcversions",
  "version": "2.0.0",
  "description": "Query Minecraft versions.",
  "repository": {
    "type": "git",
    "url": "https://github.com/RyanTheAllmighty/mcversions.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RyanTheAllmighty/mcversions/issues"
  },
  "homepage": "https://github.com/RyanTheAllmighty/mcversions",
  "keywords": [
    "minecraft"
  ],
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib"
  },
  "author": "Ryan Dowling <ryan@ryandowling.me>",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prebuild": "rimraf dist/",
    "prepublishOnly": "npm run build",
    "release": "np"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add -f"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "lru-cache": "^5.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/axios": "^0.14.0",
    "@types/lru-cache": "^5.1.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.1",
    "np": "^5.0.3",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
